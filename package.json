    ".": {
      "import": "./dist/flipbook.js",
      "require": "./dist/flipbook.umd.js",
      "default": "./dist/flipbook.js",
      "types": "./types/flipbook.d.ts"
    }
  },
  "devDependencies": {
    "postcss": "*",
    "autoprefixer": "*",
    "vite": "*",
    "@vitejs/plugin-vue": "*",
    "vite-plugin-css-injected-by-js": "*"
  },
  "peerDependencies": {
    "vue": ">=3.4.38"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "rematrix": "*"
  }
}
